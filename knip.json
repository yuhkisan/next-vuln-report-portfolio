{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["app/**/*.{ts,tsx}"],
  "project": ["app/**/*.{ts,tsx}", "lib/**/*.ts", "prisma/**/*.ts"],
  "ignore": ["**/*.d.ts", "generated/**"],
  "ignoreDependencies": [
    "@prisma/client",
    "@eslint/eslintrc",
    "@eslint/js",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "eslint-plugin-react",
    "eslint-plugin-react-hooks",
    "globals"
  ]
}
